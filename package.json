{
  "name": "babel-dts-generator",
  "version": "0.3.0",
  "description": "Babel DTS generator",
  "main": "lib/index.js",
  "scripts": {
    "build": "node node_modules/babel/bin/babel.js src --out-dir lib --source-maps --stage 1",
    "test": "npm run build && node node_modules/babel/bin/babel-node.js test/index.js",
    "watch": "node node_modules/nodemon/bin/nodemon.js --exec \"npm run test\" --watch src --watch spec --watch test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YoloDev/babel-dts-generator.git"
  },
  "keywords": [
    "aurelia",
    "babel",
    "dts"
  ],
  "author": "Alxandr",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/YoloDev/babel-dts-generator/issues"
  },
  "homepage": "https://github.com/YoloDev/babel-dts-generator#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.30",
    "babel-eslint": "^4.1.3",
    "cli-color": "^1.1.0",
    "diff": "^2.1.3",
    "eslint": "^1.7.1",
    "glob-promise": "^1.0.4",
    "nodemon": "^1.7.1",
    "typescript": "^1.6.2"
  }
}
